{
    "manifest_version": 3,
    "name": "ID Capture Extension",
    "version": "1.0",
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "permissions": [
        "webRequest",
        "storage",
        "scripting",
        "activeTab"
    ],
    "host_permissions": [
        "<all_urls>",
        "*://*/*",
        "*://*.example.org/"
    ]
}